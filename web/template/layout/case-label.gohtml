{{ define "CaseLabel" }}
    {{ if eq .CaseType "EPA" }}
        <strong class="app-govuk-tag {{ caseLabel .CaseType }}">{{ ToUpper .CaseType }}</strong> {{ .UID }}
    {{ else }}
        <strong class="app-govuk-tag {{ caseLabel .SubType }}">{{ ToUpper .SubType }}</strong> {{ .UID }}
    {{ end }}
{{ end }}